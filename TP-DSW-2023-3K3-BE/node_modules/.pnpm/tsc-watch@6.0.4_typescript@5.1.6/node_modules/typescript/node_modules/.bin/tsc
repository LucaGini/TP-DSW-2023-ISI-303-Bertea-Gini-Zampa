#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Usuario/OneDrive/Escritorio/ISI UTN 3ero/Desarrollo de Software/TP-DSW-2023-3K3/TP-DSW-2023-3K3-BE/node_modules/.pnpm/typescript@5.1.6/node_modules/typescript/bin/node_modules:/mnt/c/Users/Usuario/OneDrive/Escritorio/ISI UTN 3ero/Desarrollo de Software/TP-DSW-2023-3K3/TP-DSW-2023-3K3-BE/node_modules/.pnpm/typescript@5.1.6/node_modules/typescript/node_modules:/mnt/c/Users/Usuario/OneDrive/Escritorio/ISI UTN 3ero/Desarrollo de Software/TP-DSW-2023-3K3/TP-DSW-2023-3K3-BE/node_modules/.pnpm/typescript@5.1.6/node_modules:/mnt/c/Users/Usuario/OneDrive/Escritorio/ISI UTN 3ero/Desarrollo de Software/TP-DSW-2023-3K3/TP-DSW-2023-3K3-BE/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Usuario/OneDrive/Escritorio/ISI UTN 3ero/Desarrollo de Software/TP-DSW-2023-3K3/TP-DSW-2023-3K3-BE/node_modules/.pnpm/typescript@5.1.6/node_modules/typescript/bin/node_modules:/mnt/c/Users/Usuario/OneDrive/Escritorio/ISI UTN 3ero/Desarrollo de Software/TP-DSW-2023-3K3/TP-DSW-2023-3K3-BE/node_modules/.pnpm/typescript@5.1.6/node_modules/typescript/node_modules:/mnt/c/Users/Usuario/OneDrive/Escritorio/ISI UTN 3ero/Desarrollo de Software/TP-DSW-2023-3K3/TP-DSW-2023-3K3-BE/node_modules/.pnpm/typescript@5.1.6/node_modules:/mnt/c/Users/Usuario/OneDrive/Escritorio/ISI UTN 3ero/Desarrollo de Software/TP-DSW-2023-3K3/TP-DSW-2023-3K3-BE/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsc" "$@"
else
  exec node  "$basedir/../../bin/tsc" "$@"
fi
